<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.godsky.findlove.common.model.vo.User" alias="User" />
		<typeAlias type="com.godsky.findlove.common.model.vo.Profile" alias="Profile" />
		<typeAlias type="com.godsky.findlove.admin.usermanagement.model.vo.UserMgmt" alias="UserMgmt" />
		<typeAlias type="com.godsky.findlove.admin.usermanagement.model.vo.UserBasic" alias="UserBasic" />
		<typeAlias type="com.godsky.findlove.admin.usermanagement.model.vo.UserDetail" alias="UserDetail" />
		<typeAlias type="com.godsky.findlove.common.model.vo.Profile" alias="Profile" />
		<typeAlias type="com.godsky.findlove.common.model.vo.ProfilePicture" alias="ProfilePicture" />
		<typeAlias type="com.godsky.findlove.common.model.vo.Report" alias="Report" />
	</typeAliases>
	
	<!-- 사용할 매퍼 파일 등록 : 필수 -->
