<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.godsky.findlove.user.model.vo.User" alias="User" />
		<typeAlias type="com.godsky.findlove.main.profileboard.model.vo.Message" alias="Message" />
		<typeAlias type="com.godsky.findlove.main.profileboard.model.vo.Profileboard" alias="Profileboard" />
		<typeAlias type="com.godsky.findlove.main.noticeboard.model.vo.Notice" alias="Notice" />
		<typeAlias type="com.godsky.findlove.admin.usermanagement.model.vo.UserBasic" alias="UserBasic" />
		<typeAlias type="com.godsky.findlove.admin.usermanagement.model.vo.UserDetail" alias="UserDetail" />
		<typeAlias type="com.godsky.findlove.common.model.vo.Profile" alias="Profile" />
		<typeAlias type="com.godsky.findlove.common.model.vo.Report" alias="Report" />
		<typeAlias type="com.godsky.findlove.main.eventboard.poll.model.vo.Poll" alias="Poll" />
		<typeAlias type="com.godsky.findlove.main.eventboard.poll.model.vo.PollAnswer" alias="PollAnswer" />
		<typeAlias type="com.godsky.findlove.main.eventboard.poll.model.vo.PollResult" alias="PollResult" />
		<typeAlias type="com.godsky.findlove.main.eventboard.attendance.model.vo.Attendance" alias="Attendance" />
	</typeAliases>
	
	<!-- 사용할 매퍼 파일 등록 : 필수 -->
	<!-- 주의 : 등록된 매퍼 파일중에 오류가 있으면, 다른 매퍼에도 영향을 줌 -->
	<mappers>
		<mapper resource="mapper/user-mapper.xml" />
		<mapper resource="mapper/profile-mapper.xml" />	
	 	<mapper resource="mapper/message-mapper.xml" /> 
	 	<mapper resource="mapper/report-mapper.xml" /> 
	 	<mapper resource="mapper/point-mapper.xml" />
	 	<mapper resource="mapper/konpeito-mapper.xml" />
	 	<mapper resource="mapper/notice-mapper.xml" />
	 	<mapper resource="mapper/user-mgmt-mapper.xml" />
	 	<mapper resource="mapper/poll-mapper.xml" />
	 	<mapper resource="mapper/attendance-mapper.xml" />			
	 		
	</mappers>
</configuration>















